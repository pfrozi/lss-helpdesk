if user.type=="D"
    extends ../menus/developer
else if user.type="C"
    extends ../menus/customer
else
    extends ../menus/customer


block content
    h1.
        ID: #{user._id}
    p Name: #{user.name}
    p Nick: #{user.nick}
    p Password: #{user.password}
    p Created at: #{user.created}

    
